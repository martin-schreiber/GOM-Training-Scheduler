<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.gomscheduler"> <!-- Replace with your actual package name -->

    <application
        android:allowBackup="true"
        android:icon="@drawable/bulb_high_res"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bulb_high_res"
        android:supportsRtl="true"
        android:theme="@style/Theme.GOMScheduler">
        <activity
            android:name=".AutoSchedule"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddSkill"
            android:exported="false" />
        <activity
            android:name=".SubSkill"
            android:exported="false" />
    </application>

</manifest>
